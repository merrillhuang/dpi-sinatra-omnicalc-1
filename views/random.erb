<form action="/random/results">
  <div>
    <label for="min_input">Minimum</label>
    <input type="text" name="min" id="min_input" placeholder="E.g. 1.5">
  </div>
  <div>
    <label for="max_input">Maximum</label>
    <input type="text" name="max" id="max_input" placeholder="E.g. 4.5">
  </div>
  <button>Pick random number</button>
</form>
